    "import numba as nb\n",
    "import numpy as np\n",
    "\n",
    "\n",
    "@nb.njit\n",
    "def fn(n) -> int:\n",
    "    if n <= 0:\n",
    "        return n\n",
    "    n -= 1\n",
    "    return fn(n)\n",
    "\n",
    "x = np.array([50])\n",
    "print(fn(x))\n",
    "x"
   ]
